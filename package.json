{
  "name": "acorn-web",
  "version": "1.0.0",
  "description": "A modern web component library built with Lit",
  "type": "module",
  "exports": {
    "./components/*": {
      "import": "./dist/components/*/*.mjs"
    },
    "./styles/*": {
      "import": "./dist/styles/*.css"
    },
    "./assets/*": "./dist/assets/*"
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "keywords": [
    "web-components",
    "lit",
    "esm",
    "components"
  ],
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FirefoxUX/acorn-web.git"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@fluent/bundle": "^0.19.1",
    "@fluent/dom": "^0.10.2",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-links": "^9.0.18",
    "@storybook/web-components-vite": "^9.0.18",
    "storybook": "^9.0.18",
    "vite": "^5.0.0"
  }
}
